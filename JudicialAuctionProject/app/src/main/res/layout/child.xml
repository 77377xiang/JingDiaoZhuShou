<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:background="#FFFFFF" >



<LinearLayout
    android:layout_marginLeft="30dp"
    android:layout_marginRight="30dp"
    android:gravity="center"
    android:orientation="horizontal"
    android:layout_width="match_parent"
    android:layout_height="40dp">
    <TextView
        android:id="@+id/t_sun_tv"
        android:layout_weight="1"
        android:gravity="center"
        android:text="成交件数"
        android:textSize="12dp"
        android:textColor="#929292"
        android:layout_width="0dp"
        android:layout_height="wrap_content" />
    <TextView
        android:id="@+id/t_fix_tv"
        android:layout_weight="1"
        android:gravity="center"
        android:text="上拍件数"
        android:textSize="12dp"
        android:textColor="#929292"
        android:layout_width="0dp"
        android:layout_height="wrap_content" />
    <TextView
        android:id="@+id/t_money_tv"
        android:layout_weight="1"
        android:gravity="center"
        android:text="成交价格"
        android:textSize="12dp"
        android:textColor="#929292"
        android:layout_width="0dp"
        android:layout_height="wrap_content" />
    <TextView
        android:id="@+id/t_proportion_tv"
        android:layout_weight="1"
        android:gravity="center"
        android:text="成交率"
        android:textSize="12dp"
        android:textColor="#929292"
        android:layout_width="0dp"
        android:layout_height="wrap_content" />


</LinearLayout>


    <LinearLayout
        android:layout_marginLeft="30dp"
        android:layout_marginRight="30dp"
        android:gravity="center"
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="40dp">
        <TextView
            android:id="@+id/t_sun2_tv"
            android:layout_weight="1"
            android:gravity="center"
            android:text="所有"
            android:textSize="12dp"
            android:textColor="#929292"
            android:layout_width="0dp"
            android:layout_height="wrap_content" />
        <TextView
            android:id="@+id/t_fix2_tv"
            android:layout_weight="1"
            android:gravity="center"
            android:text="4000"
            android:textSize="12dp"
            android:textColor="#929292"
            android:layout_width="0dp"
            android:layout_height="wrap_content" />
        <TextView
            android:id="@+id/t_money2_tv"
            android:layout_weight="1"
            android:gravity="center"
            android:text="1200"
            android:textSize="12dp"
            android:textColor="#929292"
            android:layout_width="0dp"
            android:layout_height="wrap_content" />
        <TextView
            android:id="@+id/t_proportion2_tv"
            android:layout_weight="1"
            android:gravity="center"
            android:text="30%"
            android:textSize="12dp"
            android:textColor="#929292"
            android:layout_width="0dp"
            android:layout_height="wrap_content" />


    </LinearLayout>



    <LinearLayout
        android:layout_marginLeft="30dp"
        android:layout_marginRight="30dp"
        android:gravity="center"
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="40dp">
        <TextView
            android:id="@+id/t_sun3_tv"
            android:layout_weight="1"
            android:gravity="center"
            android:text="一拍"
            android:textSize="12dp"
            android:textColor="#929292"
            android:layout_width="0dp"
            android:layout_height="wrap_content" />
        <TextView
            android:id="@+id/t_fix3_tv"
            android:layout_weight="1"
            android:gravity="center"
            android:text="1000"
            android:textSize="12dp"
            android:textColor="#929292"
            android:layout_width="0dp"
            android:layout_height="wrap_content" />
        <TextView
            android:id="@+id/t_money3_tv"
            android:layout_weight="1"
            android:gravity="center"
            android:text="500000"
            android:textSize="12dp"
            android:textColor="#929292"
            android:layout_width="0dp"
            android:layout_height="wrap_content" />
        <TextView
            android:id="@+id/t_proportion3_tv"
            android:layout_weight="1"
            android:gravity="center"
            android:text="69%"
            android:textSize="12dp"
            android:textColor="#929292"
            android:layout_width="0dp"
            android:layout_height="wrap_content" />


    </LinearLayout>
    <LinearLayout
        android:layout_marginLeft="30dp"
        android:layout_marginRight="30dp"
        android:gravity="center"
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="40dp">
        <TextView
            android:id="@+id/t_sun4_tv"
            android:layout_weight="1"
            android:gravity="center"
            android:text="二拍"
            android:textSize="12dp"
            android:textColor="#929292"
            android:layout_width="0dp"
            android:layout_height="wrap_content" />
